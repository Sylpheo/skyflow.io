{% extends "layout.html.twig" %}

{% block title %}Email{% endblock %}

{% block content %}
<div class="tab-content">
    <div class="tab-pane fade in active" >        
        <div class="table-responsive">
            <table class="table table-hover table-condensed viewTable" id="email">
                <thead>
                    <tr>
                        <th>ID</th>
                        <th>Name</th>
                        <th>Subject</th>
                        <th>CustomerKey</th>
                    </tr>
                </thead>
                        <tr>
                        	<td>{{ email.ID}}</td>
                        	<td>{{email.Name}}</td>
        					<td>{{email.Subject}}</td>
        					<td>{{email.CustomerKey}}</td>
                           
        				</tr>
			</table> 
        </div>
</div>
</div>

{% endblock %}
