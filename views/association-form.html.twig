{% extends "layout.html.twig" %}

{% block title %}New mapping{% endblock %}

{% block content %}
<div class="container">
    <div class="row">
        <div class="col-lg-12">
            <h1 class="page-header">New Mapping
            </h1>
            <ol class="breadcrumb">
                <li><a href="/">Home</a></li>
                <li><a href="mapping">Mapping</a></li>
                <li class="active">Create</li>
            </ol>
        </div>
    </div>

    <form action="#" method="post">
        {{form_start(associationForm)}}
        {{form_row(associationForm.event, {'label' : 'Event'}) }}
        {{form_row(associationForm.flow, {'label' : 'Flow'}) }}


        <input type="submit" name="submit" value="Add !" />
        {{form_end(associationForm)}}
    </form>
</div>
{% endblock %}
